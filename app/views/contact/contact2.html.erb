<body>
<div class="window" id="nyc_jumbo">
	<div class="container-fluid">
		<div id="contact_box">
			<div id="contact-content">
				<h2 class="pg_title">Contact Me</h2>
				<p>
					<%= link_to image_tag("email.png") + " megan@megantiu.com", "mailto:megan@megantiu.com" %>
				</p>
				<p>
					<%= link_to image_tag('my_tw2.png') + " @megandtiu", "http://twitter.com/megandtiu" %>
				</p>
				<div class="input-wrapper form-group">
		
					<%= form_tag(create_contact_path) do %>
					  <%= text_field_tag :name, nil, placeholder: "Name", class: "form-control" %>
					  <%= text_field_tag :email, nil, placeholder: "Email", class: "form-control" %>
					  <%= text_area_tag :message, nil, placeholder: "Message", class: "form-control" %>
					  <%= submit_tag "Send", class: "form-control" %>
					<% end %>

				</div>
			</div>
		</div>
	</div>
</div>
</body>